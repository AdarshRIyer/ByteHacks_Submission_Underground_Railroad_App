<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Group2Certificates">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/button62"
            android:layout_width="217dp"
            android:layout_height="66dp"
            android:layout_marginEnd="16dp"
            android:onClick="GoHome"
            android:text="Done/Home"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/button68"
            app:layout_constraintTop_toBottomOf="@+id/imageView18" />

        <Button
            android:id="@+id/button68"
            android:layout_width="138dp"
            android:layout_height="59dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="4dp"
            android:onClick="LOGOUT"
            android:text="Logout"
            app:layout_constraintEnd_toStartOf="@+id/button62"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView18" />

        <ImageView
            android:id="@+id/imageView18"
            android:layout_width="410dp"
            android:layout_height="2520dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/group2cert" />
    </android.support.constraint.ConstraintLayout>
</ScrollView>